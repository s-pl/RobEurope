{
  "source": {
    "include": ["backend", "frontend/src"],
    "includePattern": ".+\\.(js|jsx|mjs)$",
    "excludePattern": "(\\\\|/)(node_modules|__tests__|uploads|logs|dist)(\\\\|/)|(^|\\\\|/)backend(\\\\|/)public(\\\\|/)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "destination": "docs/generated",
    "recurse": true,
    "private": false
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
