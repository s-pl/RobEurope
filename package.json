{
    "name": "robeurope",
    "version": "0.1.0",
    "description": "Proyecto RobEurope",
    "main": "backend/index.js",
    "scripts": {
        "start": "node ./backend/index.js",
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "npm --prefix backend run dev",
        "dev:frontend": "npm --prefix frontend run dev",
        "build": "npm --prefix frontend run build",
        "test": "npm --prefix backend run test",
        "migrate": "npm --prefix backend run migrate",
        "seed": "npm --prefix backend run seed",
        "lint": "eslint .",
        "docs:generate": "jsdoc -c docs/jsdoc.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://example.com/robeurope.git"
    },
    "keywords": [],
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://example.com/robeurope/issues"
    },
    "homepage": "https://example.com/robeurope#readme",
    "devDependencies": {
        "clean-jsdoc-theme": "^4.3.0",
        "concurrently": "^9.1.0",
        "eslint": "^8.49.0",
        "jsdoc": "^4.0.5",
        "nodemon": "^2.0.22"
    }
}
